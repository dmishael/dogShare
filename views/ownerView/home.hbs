<h1>Welcome to Dog Share</h1>



<a href="/new"><button>Sign Up!</button></a>
<a href="/apps"><button>Check Out Appointments!</button></a>

{{#each profile}}
<div class="row">
    <div class="col s12 m6">
        <div class="card blue-grey darken-1">
            <div class="card-content white-text">
                <span class="card-title"></span>
                    <p>name - {{name}}</p>
                    <p>address - {{address}}</p>
                    <p>dog name - {{dogName}}</p>


                      
                
            </div>
            
        </div>
    </div>
</div>

{{/each}}